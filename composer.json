{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.18",
        "appzcoder/laravel-admin": "dev-master#v3.1.0",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "6.0.*",
        "laravel/tinker": "1.0.*",
        "laravel/vapor-core": "^2.0",
        "laravelcollective/html": "6.0.*"
    },
    "require-dev": {
        "appzcoder/crud-generator": "dev-master#v3.0.8",
        "fzaninotto/faker": "1.8.*",
        "mockery/mockery": "1.2.*",
        "nunomaduro/collision": "3.0.*",
        "phpunit/phpunit": "8.3.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "appzcoder/crud-generator": {
            "type": "github",
            "url": "https://github.com/authanram/crud-generator.git"
        },
        "appzcoder/laravel-admin": {
            "type": "github",
            "url": "https://github.com/authanram/laravel-admin.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
